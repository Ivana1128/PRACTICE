{
  "name": "project-tailwind-1",
  "version": "1.0.0",
  "scripts": {
    "serve": "cross-env NODE_ENV=development concurrently \"postcss dist/style.css -o dist/build/style.css --watch\"  \"live-server ./dist\"",
    "development": "cross-env NODE_ENV=development postcss dist/style.css -o dist/build/style.css",
    "production": "cross-env NODE_ENV=production postcss dist/style.css -o dist/build/style.css",
    "start": "npm run serve",
    "build:css": "tailwind build src/style.css -o dist/style.css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.0.4",
    "autoprefixer": "^9.8.6",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "live-server": "^1.2.1",
    "postcss": "^8.3.0",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.4"
  },
  "description": ""
}
